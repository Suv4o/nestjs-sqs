{
  "name": "@suv4o/nestjs-sqs",
  "version": "0.2.0",
  "description": "Custom NestJS microservice transport strategy for AWS SQS with native @MessagePattern support.",
  "keywords": [
    "nestjs",
    "sqs",
    "aws",
    "microservices",
    "message-pattern",
    "transport"
  ],
  "license": "MIT",
  "author": "Suv4o",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@aws-sdk/client-sqs": ">=3",
    "@nestjs/common": ">=10",
    "@nestjs/core": ">=10",
    "@nestjs/microservices": ">=10",
    "rxjs": ">=7"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
